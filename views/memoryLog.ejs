<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <link rel="stylesheet" href="/style.css">
        <title>My Memory Collector</title>
    </head>

    <body>
        <header class="header">
            <h1 class="logo"><a href="#">Memory Collector</a></h1>
            <ul class="main-nav">
                <li><a href="/">Home</a></li>
                <li><a href="/seeLogsTime">See Memories</a></li>
            </ul>
        </header> 
        <main>    
        <div class="section">
            <label for="personNameSelected">Filter By Person: </label>
            <select name="personNameSelected" id="personNameSelected">
                <option>Select Person</option>
                <% personList.forEach(function (person) { %>
                    <option value="<%= person.name %>"><%= person.name %>
                    <% }) %>
                  </option>
            </select><br>
            <button onclick="getDataByPerson()" type="button">Apply Filter</button>
            <button onclick="getDataByTime()" ntype="button">Remove Filter</button>
        </div>
        <div class="container">
            <div id="dataByTime"></div>
            <div id="dataByPerson"></div>
        </div>
    </main>

    <script src="logs.js"></script>
    </body>
</html>